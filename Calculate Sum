#include<bits/stdc++.h>
using namespace std;
vector<long long>s(1000001);
vector<long long>f(1000001);

void f1(){
    for(int i=1;i<=1000000;i++){
        for(int j=i;j<=1000000;j+=i){
            s[j]+=i;
        }
    }
}
void f2(){
    for(int i=1;i<=1000000;i++){
        for(int j=i;j<=1000000;j+=i){
            f[j]+=s[i];
        }
    }
}

int main(){
    int q;
    cin>>q;
    f1();
    f2();
    for(int i=1;i<=1000000;i++){
        f[i]+=f[i-1];
    }
    int ctr=1;
    while(q--){
        int l,r;
        cin>>l>>r;
        if(l==0){
            cout<<f[r]<<endl;
        }
        else{
            cout<<(f[r]-f[l-1])<<endl;
        }
    }
    
    return 0;
}

